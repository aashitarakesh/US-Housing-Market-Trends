   "source": [
    "sel_cols = ['period_begin',  \n",
    "'region_name' ,\n",
    "'region_type',\n",
    "'total_homes_sold',\n",
    "'median_sale_price',\n",
    "]\n",
    "df_reduced = df[sel_cols]\n",
    "df_reduced = df_reduced[df_reduced[\"region_type\"]==\"county\"]\n",
    "df_reduced = df_reduced.drop(columns = [\"region_type\"])\n",
    "df_reduced"
   ]
