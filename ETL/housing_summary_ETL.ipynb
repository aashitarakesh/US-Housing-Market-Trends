    "\n",
    "df_reduced[\"year_month\"] = begin_dates.dt.strftime(\"%Y-%m\")\n",
    "\n",
    "states = []\n",
    "for region in df_reduced[\"region_name\"]:\n",
    "    reg_list = region.split(\",\")\n",
    "    if len(reg_list) == 2:\n",
    "        reg_state = reg_list[1][1:3]\n",
    "        states.append(reg_list[1][1:3])\n",
    "    else:\n",
    "        states.append(\"\")\n",
    "df_reduced[\"state\"] = states\n"
